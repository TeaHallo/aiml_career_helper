    <category>
        <that>Would you like assistance with choosing a career path</that>
        <pattern>YES</pattern>
        <template>
        Great! I will ask you 25 questions.<split/><delay>2</delay>
        After clicking on your final answer, click the Next button.<split/>
      <think><srai>set</srai></think>
      <delay>5</delay>
      <srai>Ready</srai>
        </template>
    </category>
    
    <category>
